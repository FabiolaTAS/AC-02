<?xml version="1.0" encoding="utf-8"?>

<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".TelaInicialActivity"
    android:id="@+id/layoutMenuLateral"
    android:fitsSystemWindows="true">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <include layout="@layout/toolbar"/>

          <TextView
            android:id="@+id/textoInicial"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.106" />

        <Button
            android:id="@+id/button_sair"
            android:layout_width="298dp"
            android:layout_height="44dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintVertical_bias="0.928" />

        <Button
            android:id="@+id/bt_cadastrarMulher"
            android:layout_width="224dp"
            android:layout_height="51dp"
            app:layout_constraintBottom_toTopOf="@+id/button_sair"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.518"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textoInicial"
            app:layout_constraintVertical_bias="0.612" />


        <ProgressBar
            android:id="@+id/atualizando"
            android:layout_width="369dp"
            android:layout_height="230dp"
            app:layout_constraintBottom_toTopOf="@+id/bt_cadastrarMulher"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/bt_cadastrarComunidades"
            app:layout_constraintVertical_bias="0.504"
            tools:visibility="invisible"
            />

        <Button
            android:id="@+id/bt_cadastrarEmpresa"
            android:layout_width="222dp"
            android:layout_height="53dp"
            app:layout_constraintBottom_toTopOf="@+id/bt_cadastrarComunidades"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.513"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textoInicial" />

        <Button
            android:id="@+id/bt_cadastrarComunidades"
            android:layout_width="221dp"
            android:layout_height="53dp"
            app:layout_constraintBottom_toTopOf="@+id/bt_cadastrarMulher"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.526"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textoInicial"
            app:layout_constraintVertical_bias="0.756" />



</LinearLayout>

    <com.google.android.material.navigation.NavigationView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:id="@+id/menu_lateral"
        android:layout_gravity="start"
        android:fitsSystemWindows="true"
        app:headerLayout="@layout/menu_lateral_cabecalho"
        app:menu="@menu/menu_lateral"
        />

</androidx.drawerlayout.widget.DrawerLayout>



